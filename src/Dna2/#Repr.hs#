
module Dna2.Repr where

import qualified Data.Vector.Unboxed as UVec
import qualified Data.Vector.Unboxed.Mutable as MUVec
  
data Gen 
  = Inp
  | Lit Integer
  | Neg
  | Inv
  | Add
  | Mul
  | Pow
  deriving (Show, Eq{-, Enum, Bounded-})

-- newtype Genome = Codex (UVec.Vector Gen)
newtype Genome = Codex [Gen]
  deriving (Show, Eq)

compile :: Genome -> (Integer -> Integer)
compile g = undefined









